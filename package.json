{
  "name": "express-babel-starter",
  "version": "1.0.0",
  "description": "An unopinionated express starter kit with support for es6+ features using babel",
  "private": false,
  "repository": "https://github.com/benmalcom/express-babel-starter",
  "author": "Ben ikeji <benjaminikeji@gmail.com>",
  "main": "dist/app.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "node dist/app",
    "start:dev": "nodemon src/app --exec \"npm run lint && node -r babel-register\"",
    "clean": "rimraf dist",
    "build": "npm run clean && mkdir dist && babel src -s -d dist",
    "test": "mocha --exit --require babel-register test/",
    "lint": "esw src"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "body-parser": "~1.17.1",
    "config": "^1.27.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "ejs": "^2.5.7",
    "express": "~4.15.2",
    "morgan": "~1.8.1",
    "rimraf": "^2.6.2"
  },
  "keywords": [
    "express",
    "babel",
    "boilerplate",
    "scaffold",
    "es6",
    "es2015",
    "es2016",
    "es2017",
    "jest",
    "eslint"
  ],
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "babel-register": "^6.26.0",
    "eslint": "^4.10.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-watch": "^3.1.3",
    "mocha": "^4.0.1",
    "supertest": "^3.0.0"
  }
}
